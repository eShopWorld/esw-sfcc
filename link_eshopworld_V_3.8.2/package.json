  "dependencies": {
    "bootstrap": "4.3.1",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "cleave.js": "^1.6.0",
    "flag-icon-css": "^2.9.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "sgmf-scripts": "^2.4.2"
